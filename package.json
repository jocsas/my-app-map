    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "typescript": "5.0.4"
  }
